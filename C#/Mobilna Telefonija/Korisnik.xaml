<Window x:Class="MilicaZivkovic.Korisnik"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MilicaZivkovic"
        mc:Ignorable="d"
        Title="Novi korisnik" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="Ime:" HorizontalAlignment="Left" Margin="23,35,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Datum rodjenja:" HorizontalAlignment="Left" Margin="23,95,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Adresa" HorizontalAlignment="Left" Margin="23,151,0,0" VerticalAlignment="Top"/>
        <DatePicker Name="datum" HorizontalAlignment="Left" Margin="154,97,0,0" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="adresa" HorizontalAlignment="Left" Height="23" Margin="154,159,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="ime" HorizontalAlignment="Left" Height="23" Margin="154,35,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="button" Content="Dodaj" HorizontalAlignment="Left" Margin="69,210,0,0" VerticalAlignment="Top" Width="155" Click="button_Click_1"/>

    </Grid>
</Window>
