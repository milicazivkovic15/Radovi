<Window x:Class="MilicaZivkovic.Telefon"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MilicaZivkovic"
        mc:Ignorable="d"
        Title="Novi telefon" Height="300" Width="300">
    <Grid>
        <Label x:Name="label" Content="Marka" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Korisnik" HorizontalAlignment="Left" Margin="10,105,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label2" Content="Broj" HorizontalAlignment="Left" Margin="10,168,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox1" HorizontalAlignment="Left" Margin="103,172,0,0" VerticalAlignment="Top" Width="171" />
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="103,109,0,0" VerticalAlignment="Top" Width="171"  DisplayMemberPath="ime"/>
        <TextBox x:Name="marka" HorizontalAlignment="Left" Height="23" Margin="103,43,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="171"/>
        <Button x:Name="button" Content="Dodaj" HorizontalAlignment="Left" Margin="181,222,0,0" VerticalAlignment="Top" Width="93" Click="button_Click"/>

    </Grid>
</Window>
